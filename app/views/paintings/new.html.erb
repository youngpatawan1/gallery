<%provide(:title, 'Sign Up') %>
<h1>Upload New Painting</h1>
<div class="row">
<div class="col-md-6 col-md-offset-3">
            <%= form_with model: @user do |f| %>
                <%= f.label :paintings %>
                <%= f.file_field :paintings, multiple: true, required: true, class: 'form-control'%>

                <%= f.submit "Save", class: "btn btn-primary"%>
                <%= link_to "Cancel", @user , class: "btn cancel-btn" %>
            <% end %>
</div>
</div>